    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Benchy with hashing.git",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Benchy/bin/Debug/net9.0/Benchy.dll",
            "args": ["Examples/hashing.git", "algs/md5", "algs/sha256"],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}